/*Criando uma tabela com o nome contas*/
CREATE TABLE contas(
	id INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
    nome VARCHAR(255),
    sobre_nome VARCHAR (255),
    saldo int,
    data_nascimento DATE
);

